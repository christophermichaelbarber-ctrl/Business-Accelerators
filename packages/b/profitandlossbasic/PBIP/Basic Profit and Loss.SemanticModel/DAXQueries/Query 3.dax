DEFINE
    MEASURE '_Measures'[Actuals] = __Fn_PnL_Sum_Subtotal_and_Blanks (sum(Ledger[Credit Less Debit @Global Fx]))
    MEASURE '_Measures'[Plan] = __Fn_PnL_Sum_Subtotal_and_Blanks (sum(Scenario[Credit less debit]))
    MEASURE '_Measures'[Var, #] = [Actuals] - [Plan]
    MEASURE '_Measures'[Var, %] = Divide([Var, #], [actuals])

EVALUATE
    SUMMARIZECOLUMNS(
        "Actuals", [Actuals],
        "Plan", [Plan],
        "Var, #", [Var, #],
        "Var, %", [Var, %]
    )